	}
	
	public MealBuilder prepareNonVegMeal() {
		MealBuilder.addItem(new ChickenBurger());
		MealBuilder.addItem(new Coke());
		return this;
	}
	
	public MealBuilder addItem(Item Item) {
		
	}
	
	public Meal Build() {
		
	}
}
